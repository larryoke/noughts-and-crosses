package gameCommon;

public class MarkOutcome {

	private MarkStatus markStatus;

	private String errorMessage;

	private String score;

	public MarkOutcome() {
		super();
	}

	public MarkOutcome(MarkStatus markStatus, String score, String errorMessage) {
		this.markStatus = markStatus;
		this.score = score;
		this.errorMessage = errorMessage;
	}

	public String getErrorMessage() {
		return errorMessage;
	}

	public void setErrorMessage(String message) {
		this.errorMessage = message;
	}

	public String getScore() {
		return score;
	}

	public void setScore(String score) {
		this.score = score;
	}

	public MarkStatus getPlayStatus() {
		return markStatus;
	}

	public void setMarkStatus(MarkStatus playStatus) {
		this.markStatus = playStatus;
	}
}
