package gameClient.view;

public enum Square {
    ONE(1), TWO(2), THREE(3), FOUR(4), FIVE(5), SIX(6), SEVEN(7), EIGHT(8), NINE(9);

    private int squareNo;
    
    private Square(int no){
	squareNo = no;
    }
    
    @Override
    public String toString() {
	if (ONE.equals(this)) {
	    return "1";

	} else if (TWO.equals(this)) {
	    return "2";

	} else if (THREE.equals(this)) {
	    return "3";

	} else if (FOUR.equals(this)) {
	    return "4";

	} else if (FIVE.equals(this)) {
	    return "5";

	} else if (SIX.equals(this)) {
	    return "6";

	} else if (SEVEN.equals(this)) {
	    return "7";

	} else if (EIGHT.equals(this)) {
	    return "8";

	} else if (NINE.equals(this)) {
	    return "9";
	}
	return "";

    }
}
