package gameClient.view;

public class Board {

    private Cell cell1 = Cell.EMPTY;
    private Cell cell2 = Cell.EMPTY;
    private Cell cell3 = Cell.EMPTY;
    private Cell cell4 = Cell.EMPTY;
    private Cell cell5 = Cell.EMPTY;
    private Cell cell6 = Cell.EMPTY;
    private Cell cell7 = Cell.EMPTY;
    private Cell cell8 = Cell.EMPTY;
    private Cell cell9 = Cell.EMPTY;

    @Override
    public String toString() {

	return "\n\n\n|" + cell1 + "|" + cell2 + "|" + cell3 + "|" + "\n" + cell4 + "|" + cell5 + "|" + cell6 + "|"
		+ "\n" + cell7 + "|" + cell8 + "|" + cell9 + "|\n";
    }

    public Cell getCell1() {
	return cell1;
    }

    public void setCell1(Cell cell1) {
	this.cell1 = cell1;
    }

    public Cell getCell2() {
	return cell2;
    }

    public void setCell2(Cell cell2) {
	this.cell2 = cell2;
    }

    public Cell getCell3() {
	return cell3;
    }

    public void setCell3(Cell cell3) {
	this.cell3 = cell3;
    }

    public Cell getCell4() {
	return cell4;
    }

    public void setCell4(Cell cell4) {
	this.cell4 = cell4;
    }

    public Cell getCell5() {
	return cell5;
    }

    public void setCell5(Cell cell5) {
	this.cell5 = cell5;
    }

    public Cell getCell6() {
	return cell6;
    }

    public void setCell6(Cell cell6) {
	this.cell6 = cell6;
    }

    public Cell getCell7() {
	return cell7;
    }

    public void setCell7(Cell cell7) {
	this.cell7 = cell7;
    }

    public Cell getCell8() {
	return cell8;
    }

    public void setCell8(Cell cell8) {
	this.cell8 = cell8;
    }

    public Cell getCell9() {
	return cell9;
    }

    public void setCell9(Cell cell9) {
	this.cell9 = cell9;
    }
}
