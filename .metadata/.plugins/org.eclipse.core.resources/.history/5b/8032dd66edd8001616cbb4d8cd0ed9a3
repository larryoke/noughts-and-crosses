package gameCommon;

public class PlayResult {

	private MarkStatus playStatus;

	private String errorMessage;

	private String score;

	public PlayResult() {
		super();
	}

	public PlayResult(MarkStatus playStatus, String score, String errorMessage) {
		this.playStatus = playStatus;
		this.score = score;
		this.errorMessage = errorMessage;
	}

	public String getErrorMessage() {
		return errorMessage;
	}

	public void setErrorMessage(String message) {
		this.errorMessage = message;
	}

	public String getScore() {
		return score;
	}

	public void setScore(String score) {
		this.score = score;
	}

	public MarkStatus getPlayStatus() {
		return playStatus;
	}

	public void setPlayStatus(MarkStatus playStatus) {
		this.playStatus = playStatus;
	}
}
